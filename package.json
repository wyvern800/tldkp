{
  "name": "tldkp",
  "version": "1.0.2",
  "main": "index.js",
  "type": "module",
  "proxy": "http://localhost:3000",
  "scripts": {
    "start": "node src/shardManager.js",
    "dev": "concurrently \"npm run dev:bot\" \"npm run dev:client\"",
    "dev:bot": "nodemon -L --config nodemon.json",
    "dev:client": "npm --prefix frontend run dev",
    "dev:api": "nodemon src/startExpress.js",
    "dev:site": "concurrently \"npm run dev:client\" \"npm run dev:api\"",
    "build": "npm --prefix frontend install && npm --prefix frontend run build && npm install",
    "stripe:webhook": "stripe listen --forward-to localhost:3000/api/stripe/webhook",
    "setup:puppeteer": "npx puppeteer browsers install chrome",
    "postinstall": "npm run setup:puppeteer"
  },
  "dependencies": {
    "@clerk/backend": "1.16.1",
    "@clerk/express": "1.3.11",
    "@clerk/shared": "2.12.1",
    "@google-analytics/data": "^5.2.0",
    "@google-cloud/storage": "^7.14.0",
    "@types/react-slick": "^0.23.13",
    "ajv": "^8.17.1",
    "axios": "1.7.7",
    "cors": "2.8.5",
    "date-fns": "4.1.0",
    "discord.js": "14.16.2",
    "dotenv": "10.0.0",
    "express": "4.21.0",
    "express-rate-limit": "7.4.1",
    "firebase-admin": "12.6.0",
    "fs": "^0.0.1-security",
    "multer": "^1.4.5-lts.1",
    "next-themes": "^0.4.3",
    "node-cache": "5.1.2",
    "node-cron": "3.0.3",
    "puppeteer": "^24.21.0",
    "react-icons": "^5.3.0",
    "react-slick": "^0.31.0",
    "rollbar": "^2.26.4",
    "slick-carousel": "^1.8.1",
    "stripe": "^18.5.0",
    "uuid": "^11.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.7"
  }
}
